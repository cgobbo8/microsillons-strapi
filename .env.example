HOST=0.0.0.0
PORT=1337
NODE_ENV=production

# APP_KEYS - Générer avec: node -e "console.log(require('crypto').randomBytes(32).toString('base64') + ',' + require('crypto').randomBytes(32).toString('base64'))"
APP_KEYS=your-app-keys-here

# JWT Secrets - Générer avec: node -e "console.log(require('crypto').randomBytes(16).toString('base64'))"
JWT_SECRET=your-jwt-secret-here
ADMIN_JWT_SECRET=your-admin-jwt-secret-here
API_TOKEN_SALT=your-api-token-salt-here

# Cloudinary
CLOUDINARY_NAME=your-cloudinary-name
CLOUDINARY_KEY=your-cloudinary-key
CLOUDINARY_SECRET=your-cloudinary-secret

# Database PostgreSQL
DATABASE_HOST=your-database-host
DATABASE_PORT=5432
DATABASE_NAME=your-database-name
DATABASE_USERNAME=your-database-username
DATABASE_PASSWORD=your-database-password
DATABASE_SSL=false